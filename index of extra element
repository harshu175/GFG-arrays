class GfG {
    public int findExtra(int a[], int b[], int n) {
        // int an=-1;
        // for(int i=0;i<n;i++){
        //   if(a[i]!=b[i]){
        //       an=i;
        //       break;
        //   }
        // }
        // return an;
        
        int i=0,j=0;
        while(i<a.length && j<b.length){
            if(a[i]==b[j]){
                i++;
                j++;
            }else{
                break;
            }
        }
        return i;
    }
}
